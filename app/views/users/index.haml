.page-header
  %h1 Leaderboard

%table.table.table-striped
  %thead
    %tr
      %th User
      %th Correct
      %th Incorrect
      %th Accuracy
  %tbody
    - @users.each do |user|
      %tr
        %td= link_to(user[:email], user_path(user[:id]))
        %td= user[:correct_votes]
        %td= user[:incorrect_votes]
        %td= user[:correct_rate]
